<!--Classification Explenatory-->
<!ELEMENT classifications (genders, divisions, shootingstyles)>
<!ELEMENT genders (gender*)>
<!ELEMENT gender (#PCDATA)>
<!ATTLIST gender
        id CDATA #REQUIRED
        name CDATA #REQUIRED>
<!ELEMENT divisions (division*)>
<!ELEMENT division (#PCDATA)>
<!ATTLIST division
        id CDATA #REQUIRED
        name CDATA #REQUIRED
        requirement CDATA #REQUIRED>
<!ELEMENT shootingstyles (shootingstyle*)>
<!ELEMENT shootingstyle (rule*)>
<!ATTLIST shootingstyle
        id CDATA #REQUIRED
        name CDATA #REQUIRED>
<!ELEMENT rule (description?)>
<!ATTLIST rule
        num CDATA #REQUIRED>
<!ELEMENT description (#PCDATA) >

<!ELEMENT station (position, max-score)>
<!ATTLIST station
    id ID #REQUIRED
    range-marshall-id IDREF #REQUIRED>
<!ELEMENT position (#PCDATA)>
<!ELEMENT max-score (#PCDATA)>
<!ELEMENT tournamentday (shot*)>
<!ATTLIST tournamentday
    number CDATA #REQUIRED
    date CDATA #REQUIRED>
<!ELEMENT shot (distance, score)>
<!ATTLIST shot
    shooter-id IDREF #REQUIRED
    try (1|2|3) #REQUIRED
    station-id IDREF #REQUIRED>
<!ELEMENT distance (#PCDATA)>
<!ELEMENT score (#PCDATA)>
